						<DistortFactor>0.15f</DistortFactor>
					</PropellerWash>
					<Buoyancy>
						<Enabled>true</Enabled>
					</Buoyancy>
					<HideInsideWater>true</HideInsideWater>
				</BoatEffects>
			</Surface>
			<UnderWater>
				<Color>0.43f 0.53f 0.57f</Color>
				<Fog>
					<Enabled>true</Enabled>
					<Color>0.01f 0.045f 0.057f</Color>
					<Density>0.001f</Density>
				</Fog>
				<GodRays>
